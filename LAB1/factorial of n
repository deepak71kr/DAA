//Iterative

#include<iostream>
using namespace std;
int main(){
    int n = 5;
    int fact = 1;
    while(n>1){
        fact *= n;
        n--;
    }
    cout << fact <<endl;
}

//Recursive

#include<iostream>
using namespace std;

int fact(int n){
    if(n<=1) return 1;
    return n * fact(n-1);
}

int main(){
    int n = 5;
    int ans = fact(n);
    cout << ans;
}
