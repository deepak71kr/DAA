#include<iostream>
using namespace std;
int main(){
    int arr[] = { 32,5,7,9,33,5,67};
    int n = sizeof(arr)/sizeof(arr[0]);
    int t = 9;
    int flag = 0;
    for(int i=0; i<n; ++i){
        if(arr[i]==t){
            flag = 1;
            break;
        }
    }
    if(flag) cout << "YES, Found\n";
    else cout <<"NOT found\n";
    
}
